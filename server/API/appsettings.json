{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CorsSettings" : {
    "AllowedOrigins": ""
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server={ServerName};Initial Catalog=rizzy-db;Persist Security Info=False;User ID={UserName};Password={Password};MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=True;Connection Timeout=60;",
    "AZURE_SQL_CONNECTIONSTRING": "Server=tcp:rizzy-app.database.windows.net,1433;Initial Catalog=rizzy-db;Encrypt=True;Authentication=Active Directory Default;"
  },
  "ImageUploadSettings": {
    "MaxFileSizeInBytes": 5242880,
    "MaxResolutionWidth": 1920,
    "MaxResolutionHeight": 1080,
    "AspectRatioWidth": 4,
    "AspectRatioHeight": 5,
    "MaximumImagesCount": 6
  }
}
